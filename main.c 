#include <stdio.h>
#include <stdlib.h>
#include <unistd.h>
#include <stdbool.h>
#include <stdint.h>
#include <string.h>

#define EXIT_SUCCESS 0
#define EXIT_FAILURE_DUE_OF_FEW_ARGS 1

int main(int agrc , char** argv){
    char *BaseFile = NULL ;

    if (argc < 2){
        fprintf(stderr, "USAGE : Main PATH");
        exit(EXIT_FAILURE_DUE_OF_FEW_ARGS);
    }

    BaseFile = (char *)malloc(sizeof(char)*(strlen[argv[1]] + 1));
    exit(EXIT_SUCCESS);
}

